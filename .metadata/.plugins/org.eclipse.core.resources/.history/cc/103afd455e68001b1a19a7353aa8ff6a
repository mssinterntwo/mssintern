package collections;
import java.util.*;
public class HashMap1 {

	public static void main(String[] args) {
		
		HashMap map= new HashMap();
		map.put(1,"one");
		map.put(2,"two");
		Set s=map.entrySet();
		Iterator i=s. iterator();
		while(i.hasNext());
		{
			Map.Entry m1=(Map.Entry)i.next();
			System.out.println(m1.getKey()+" "+m1.getValue());
		}
	}

}
   