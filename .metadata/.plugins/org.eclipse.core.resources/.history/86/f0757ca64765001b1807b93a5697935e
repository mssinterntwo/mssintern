import java.util.Scanner;
public class pyramid {
	public static void main(String[]  args)
	{
		Scanner sc=new Scanner(System.in);
		int i,a,b,c,n=10,j;
		for(i=1;i<=n;i++)    
	    {    
	        a=0;    
	        b=1;    
	        System.out.println(b+"\t");    
	        for(j=1;j<i;j++)    
	        {    
	            c=a+b;    
	            System.out.println(c+"\t");    
	            a=b;    
	            b=c;    
	    
	        }    
		
	}

}
}
